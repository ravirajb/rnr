    "mongoose": "^6.1.6",
    "pino": "^7.6.3",
    "pino-pretty": "^7.3.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@prisma/client": "^4.11.0",
    "@types/mongoose": "^5.11.97",
